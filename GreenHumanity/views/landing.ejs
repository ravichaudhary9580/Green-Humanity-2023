      }).then((async (res) => {
        console.log(await res)
        const data = await res.json()

        if (res.status == 200) {
          var login = document.getElementById('login')
          var register = document.getElementById('reg')
          var reg1 = document.getElementById('regu')
          login.remove()
          register.remove()
          regu.remove()
