    <script>
        var modal;
        function init_modal() {
            const modal_element = document.getElementById("staticBackdrop")
            modal = new te.Modal(modal_element);
        }
        function closeModal() {
            modal.toggle()
        }

        function addToCart(id){
            var id = id          
            const modal_body = document.getElementById("modal_body")
            if(id.length < 2){
                modal_body.textContent = "Something went wrong"
                modal.show()
                loginErr = true
                return
            }
            var body = JSON.stringify({ id: id })
            
            fetch('/api/user/addCart', {
                method: 'post',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: body

            }).then((async (res) => {
                const data = await res.json()

                const staticBackdropLabel = document.getElementById("staticBackdropLabel")
                if (res.status != 200) {

                    modal_body.textContent = "Failed to add item in cart"
                    modal.show()
                    loginErr = true;

                } else {
                    modal_body.textContent = "You have added " + data.name +",now you currently have " + data.quantity +" in cart."
                    modal.show()
                }



            })).catch((err) => {
                console.log("Internal server error 500 -> " + err);
            })

        }
        function checkLogin() {

            fetch('/api/auth/isloggedin', {
            }).then((async (res) => {
                const data = await res.json()

                if (res.status != 200) {
                    window.location.href = '/login...'
                } else {
                    var obj_fullname = document.getElementById('fullname')
                    var obj_email = document.getElementById('email')
                    obj_fullname.textContent = data.firstname + " " + data.lastname
                    if(data.email == "null"){
                        obj_email.textContent = data.phone
                    } else obj_email.textContent = data.email
                }



            })).catch((err) => {
                console.log("Internal server error 500 -> " + err);
            })
        }
        function scrollabout() {
            
            var element = document.getElementById("itemView");

            element.scrollIntoView({ behavior: "smooth" });
        }
        async function logout() {
            fetch('/api/auth/logout', {
                method: 'post',

            }).then((async (res) => {
                window.location.href = "/login"
            })).catch((err) => {
                console.log("Internal server error 500 -> " + err);
            })

        }
        checkLogin()

    </script>
